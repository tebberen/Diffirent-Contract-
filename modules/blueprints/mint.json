{
  "name": "MintModule",
  "version": "1.0.0",
  "category": "token",
  "moduleType": "asset",
  "premium": true,
  "inputSchema": {
    "type": "object",
    "properties": {
      "supply": { "type": "string", "pattern": "^\\d+$" },
      "price": { "type": "string", "pattern": "^\\d+$" },
      "currency": { "type": "string", "format": "address" },
      "uri": { "type": "string" }
    },
    "required": ["supply", "uri"],
    "additionalProperties": false
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "tokenAddress": { "type": "string", "format": "address" },
      "mintCount": { "type": "number" }
    }
  },
  "eventSchema": [
    {
      "name": "Minted",
      "parameters": [
        { "name": "minter", "type": "address", "indexed": true },
        { "name": "tokenId", "type": "uint256", "indexed": false },
        { "name": "pricePaid", "type": "uint256", "indexed": false }
      ]
    }
  ],
  "abiAssumptions": [
    "function mint(address to, uint256 quantity) external payable",
    "function totalMinted() external view returns (uint256)"
  ],
  "exampleUsage": "Creator, MintModule ile 100 adet erişim NFT'si tanımlar ve MainHub komisyonu otomatik düşer.",
  "integrationNotes": "NFT meta verisi IPFS/Celo NFT Storage üzerinde barındırılır; royalty verileri Talent Protocol profil kartlarına işlenir."
}
